<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.spendysenseapp.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.spendysenseapp.ui.dashboard.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.spendysenseapp.ui.notifications.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" />

    <fragment
        android:id="@+id/navigation_analytics"
        android:name="com.example.spendysenseapp.ui.analytics.AnalyticsFragment"
        android:label="Analytics"
        tools:layout="@layout/fragment_analytics" />

    <fragment
        android:id="@+id/navigation_AddTransaction"
        android:name="com.example.spendysenseapp.ui.AddTransaction.AddTransactionFragment"
        android:label="Analytics"
        tools:layout="@layout/fragment_add_transaction" />

    <fragment
        android:id="@+id/navigation_ViewTransaction"
        android:name="com.example.spendysenseapp.ui.ViewTransaction.ViewTransactionFragment"
        android:label="Analytics"
        tools:layout="@layout/fragment_view_transaction" />


    <fragment
        android:id="@+id/navigation_UserProfiles"
        android:name="com.example.spendysenseapp.ui.UserProfile.UserProfileFragment"
        android:label="Analytics"
        tools:layout="@layout/fragment_user_profile" />



</navigation>